{
  "name": "vuewrapper",
  "version": "0.4.0",
  "description": "An alternative way of wrapping vue 3 component",
  "main": "./dist/vueWrapper.umd.js",
  "module": "./dist/vueWrapper.mjs",
  "exports": {
    ".": {
      "import": "./dist/vueWrapper.mjs",
      "require": "./dist/vueWrapper.umd.js"
    }
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build": "vite build"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "unplugin-vue-define-options": "^1.2.1",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.12"
  }
}
